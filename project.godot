[layer_names]

3d_physics/layer_1="world"
3d_physics/layer_2="player"
3d_physics/layer_3="ghost"
3d_physics/layer_4="door"
3d_physics/layer_6="evidence colliders"
3d_physics/layer_7="ghost collider"
3d_physics/layer_8="physics object"

[physics]

3d/run_on_separate_thread=true
common/physics_ticks_per_second=50
common/max_physics_steps_per_frame=16
3d/physics_engine="JoltPhysics3D"
jolt_3d/collisions/areas_detect_static_bodies=true
jolt_3d/limits/max_temporary_memory=128

[rendering]

lights_and_shadows/directional_shadow/soft_shadow_filter_quality=0
lights_and_shadows/positional_shadow/soft_shadow_filter_quality=3
lights_and_shadows/positional_shadow/soft_shadow_filter_quality.mobile=3
anti_aliasing/screen_space_roughness_limiter/enabled=false
scaling_3d/mode=2
scaling_3d/scale=0.75
scaling_3d/fsr_sharpness=1.0
global_illumination/sdfgi/probe_ray_count=2
global_illumination/sdfgi/frames_to_converge=1
occlusion_culling/use_occlusion_culling=true
anti_aliasing/canvas_textures/default_texture_repeat=1
