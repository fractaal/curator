config_version=5

[application]

config/name="ReasoningEngine"
run/main_scene="res://levels/node_3d.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
boot_splash/image="res://Splash.png"

[audio]

driver/enable_input=true

[autoload]

EventBus="*res://Scripts/EventBus.cs"
Config="*res://Scripts/Config.cs"
Curator="*res://Curator/Curator.tscn"
LLMInterface="*res://DeclarativeGameInterface/LLMInterface.cs"
TargetResolution="*res://Scripts/TargetResolution.cs"
VoiceWebServer="*res://Scripts/VoiceWebServer.cs"
EndgameHandler="*res://Scripts/EndgameHandler.cs"
Logger="*res://Scripts/Logger.cs"
ToastNotifications="*res://Scripts/ToastNotifications.cs"
InteractableRegistry="*res://Scripts/InteractableRegistry.cs"
GhostData="*res://Scripts/GhostData.gd"
NarrativeIntegrity="*res://Scripts/NarrativeIntegrity.cs"
AltEnter="*res://AltEnter.gd"

[display]

window/size/viewport_width=1600
window/size/viewport_height=900
window/dpi/allow_hidpi=false

[dotnet]

project/assembly_name="G4fps"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_whisper/plugin.cfg")

[filesystem]

import/blender/enabled=false

